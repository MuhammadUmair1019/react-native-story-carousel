{
  "name": "react-native-story-carousel",
  "version": "1.1.1",
  "description": "A React Native library for creating interactive and engaging screen stories",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "react-native": "src/index.tsx",
  "types": "lib/typescript/index.d.ts",
  "source": "src/index",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "test": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "prettier:write": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "dev": "watch 'npm run prepare' ./src",
    "prepare": "bob build && husky install",
    "clean": "del-cli lib"
  },
  "keywords": [
    "react",
    "react-native",
    "ios",
    "android",
    "react-native-story-carousel",
    "carousel",
    "story",
    "stories",
    "scroll",
    "swipe",
    "images",
    "snap",
    "slider",
    "umair-dev"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muhammadumair1019/react-native-story-carousel.git"
  },
  "author": "Muhammad Umair <muhammad.umair1019@gmail.com> (https://github.com/muhammadumair1019)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muhammadumair1019/react-native-story-carousel/issues"
  },
  "homepage": "https://github.com/muhammadumair1019/react-native-story-carousel#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-angular": "^18.4.3",
    "@react-native/eslint-config": "^0.74.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "conventional-changelog-angular": "^7.0.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.0",
    "jest": "^29.6.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-native": "^0.73.1",
    "react-native-builder-bob": "^0.23.2",
    "react-native-reanimated": "^3.6.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.3",
    "watch": "^1.0.2"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-native": "*",
    "react": "^18",
    "react-native": "^0.73.1",
    "react-native-reanimated": ">=3.6.1"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-native": {
      "optional": true
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
